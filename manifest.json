{
  "manifest_version": 3,
  "name": "Custom Doodle",
  "version": "1.0",
  "description": "Change Google Doodle",
  "action": {
    "default_popup": "index.html",
    "icons":  "img/logo.png"

  
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*",
        "https://*.google.com.vn/*"
      ],
      "js": [
        "content.js",
        "popup.js",
        "changeTab.js",
        "size.js"
      ]
    }
  ]
}